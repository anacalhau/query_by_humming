   "source": [
    "## Chromagram Extraction Algorithm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "96baa81e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def chromagram_extraction(audio, frameSize, hopSize):\n",
    "    # Melody Extraction\n",
    "    pitch, pitchConf, time = melody_extraction(audio, frameSize, hopSize)\n",
    "    # Convert to cents\n",
    "    pitch_cents = hz_to_cents(pitch, fref=ref_note_hz)\n",
    "    # Quantization\n",
    "    pitch_cents_q = quantization_semitone(pitch_cents)\n",
    "    # Median Filter\n",
    "    pitch_cents_q_filtered=scipy.signal.medfilt(pitch_cents_q, median_filter_size)\n",
    "    # Chromagram\n",
    "    pitch_semitones_q, chromagram = semitone_to_octave(pitch_cents_q_filtered, pitchConf)\n",
    "    # Chromagram reduction\n",
    "    chromagram_reduced, time_reduced = chromagram_reduction(chromagram, block_chromagram, time=time)\n",
    "    \n",
    "    return chromagram_reduced, time_reduced"
   ]
  },
